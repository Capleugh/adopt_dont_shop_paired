<%= form_tag "/shelters/#{@shelter.id}" do %>
  <%= label_tag :title %>
  <%= text_field_tag :title %><br/>

  <%= label_tag :rating %>
  <%= text_field_tag :rating %><br/>

  <%= label_tag :content %>
  <%= text_field_tag :content %><br/>

  <%= label_tag :optional_image %>
  <%= text_field_tag :opt_pic %><br/>

  <%= submit_tag 'Submit' %>
<% end %>
